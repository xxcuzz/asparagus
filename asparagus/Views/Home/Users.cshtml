<div class="users-block" >
    <ul id="user-ul">
        @if (Model != null) {
            @foreach (var user in Model) {
                <li>
                    @user.UserName.Substring(0, user.UserName.IndexOf('@'))
                    ел спаржу @user.Counter раз
                </li>
            }
        }
    </ul>
</div>